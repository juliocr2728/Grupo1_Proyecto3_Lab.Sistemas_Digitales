module Deco_Mux_Sumadores(
    input [3:0] Entradas,
    output reg [1:0] Salidas
    );

always @* begin
	if (Entradas == 4'b0001)
		Salidas = 2'b00;

	if (Entradas == 4'b0010)
		Salidas = 2'b01;
		
	if (Entradas == 4'b0100)
		Salidas = 2'b10;
		
	if (Entradas == 4'b1000)
		Salidas = 2'b11;
	
end


endmodule
